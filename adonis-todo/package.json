    "@adonisjs/hash": "^9.0.5",
    "@adonisjs/inertia": "^3.1.0",
    "@adonisjs/lucid": "^21.5.1",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@inertiajs/react": "^2.0.3",
    "@tanstack/react-query": "^5.66.0",
    "@vinejs/vine": "^3.0.0",
    "better-sqlite3": "^11.8.1",
    "edge.js": "^6.2.0",
