    "@japa/expect": "^3.0.3",
    "@japa/openapi-assertions": "^0.1.1",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "1.10.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "c8": "^10.1.3",
    "eslint": "^9.17.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.50.1",
